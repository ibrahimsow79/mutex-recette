{
  "Version": "2012-10-17",
  "Statement": [
    {
		"Effect": "Allow",
		"Action": [
			"s3:ListBucket",
			"s3:PutObject",
			"s3:GetObject",
			"s3:DeleteObject",
			"ec2:Describe*",
			"ec2:CreateSnapshot",
			"ec2:CreateImage"
		],
		"Resource": [
			"*.*"
		]
    },
    {
		"Effect": "Allow",
		"Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject"
		],
		"Resource": [
			"*.*"
		]
    }
	,
	{     
		"Effect": "Allow",
		"Action": [
			"ec2:Describe*",
			"ec2:CreateSnapshot",
			"ec2:CreateImage"
		],
		"Resource": [
			"*"
		]
	}
  ]
}